<div class="container">
<h2>login</h2>

<%= form_for :session, url: sessions_path do |f| %>

    <p>
      <%= f.label :email %>
      <%= f.text_field :email %>
    </p>

    <p>
      <%= f.label :password %>
      <%= f.password_field :password %>
    </p>

    <p>
      <%= f.submit class: 'button', value: 'Sign in' %>
    </p>
  <% end %>
